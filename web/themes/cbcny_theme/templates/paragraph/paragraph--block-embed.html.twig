{% set block_name = paragraph.field_block_name.target_id %}

{%
  set classes = [
    'section',
    'section--' ~ paragraph.bundle|clean_class,
    view_mode ? 'paragraph-view-mode--' ~ view_mode|clean_class,
  ]
%}

<section {{ attributes.addClass(classes) }}>
  {{ drupal_block(block_name) }}
</section>
