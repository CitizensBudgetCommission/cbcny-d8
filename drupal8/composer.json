{
  "name": "forumone/cbcny",
  "description": "Citizens Budget Commission",
  "type": "project",
  "repositories": [
    {
      "type": "path",
      "url": "web/modules/custom/*"
    },
    {
      "type": "vcs",
      "url": "git@github.com:ohthehugemanatee/DrupalDriver.git"
    },
    {
      "type": "package",
      "package": {
        "name": "harvesthq/chosen",
        "version": "1.5.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.5.1/chosen_v1.5.1.zip",
          "type": "zip"
        }
      }
    },
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "vcs",
      "url": "https://github.com/backupmigrate/backup_migrate_core"
    }
  ],
  "require": {
    "composer/installers": "^1.0.20",
    "drupal-composer/drupal-scaffold": "^2.0.1",
    "cweagans/composer-patches": "~1.0",
    "drupal/core": "~8",
    "drush/drush": "~8",
    "drupal/console": "~1.0",
    "drupal/admin_toolbar": "~1.0",
    "drupal/pathauto": "~1.0",
    "drupal/migrate_plus": "~3.0-dev",
    "drupal/migrate_tools": "~3",
    "drupal/twig_tweak": "~1.0",
    "drupal/paragraphs": "~1.0",
    "drupal/video_embed_field": "~1.0",
    "drupal/title": "~2.0",
    "drupal/linkit": "~5.0",
    "drupal/image_url_formatter": "~1.0",
    "drupal/chosen": "~2.0",
    "drupal/focal_point": "~1.0",
    "drupal/field_group": "~1.0",
    "harvesthq/chosen": "*",
    "drupal/allowed_formats": "~1.0",
    "drupal/honeypot": "^1",
    "drupal/search_api": "~1.0",
    "drupal/facets": "1.0-alpha11",
    "drupal/search_api_solr": "1.0",
    "drupal/webform": "5.0-beta2",
    "drupal/redirect": "1.0-alpha1",
    "drupal/xmlsitemap": "1.x-dev",
    "drupal/metatag": "1.0-beta11",
    "drupal/redis": "~1",
    "drupal/scheduler": "~1",
    "drupal/stage_file_proxy": "^1.0@alpha",
    "drupal/soundcloudfield": "~1",
    "drupal/drupal-extension": "^3.1",
    "psr/log": "dev-master",
    "drupal/backup_migrate": "^4.0@alpha",
    "nuvoleweb/drupal-behat": "^1.0",
    "drupal/drupal-driver": "dev-links-without-title-1.2 as 1.2.1",
    "drupal/advagg": "^3.4",
    "drupal/drush_delete": "^2.2"
  },
  "require-dev": {
    "behat/mink": "~1.7",
    "behat/mink-goutte-driver": "~1.2",
    "drupal/devel": "~1",
    "jcalderonzumba/gastonjs": "~1.0.2",
    "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
    "mikey179/vfsStream": "~1.2",
    "symfony/css-selector": "~2.8",
    "composer/composer": "dev-master",
    "g1a/composer-test-scenarios": "^2.1.0",
    "phpunit/phpunit": "^6",
    "symfony/phpunit-bridge": "^3.2",
    "squizlabs/php_codesniffer": "^2.8"
  },
  "replace": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ]
  },
  "extra": {
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ]
    },
    "patches": {
      "drupal/migrate_plus": {
        "Format_date migration process plugin": "https://www.drupal.org/files/issues/formatdate_process_plugin-2820490-17.patch"
      },
      "drupal/search_api": {
        "Add default display for Facets to use": "https://www.drupal.org/files/issues/2836477-search_api-views_default_deriver.patch"
      },
      "drupal/soundcloudfield": {
        "Fix deprecated use of l() in soundcloudfield formatter": "https://www.drupal.org/files/issues/soundcloudfield-fatal_error_occurs_on_all_pages-2760787-10.patch"
      }
    }
  }
}
