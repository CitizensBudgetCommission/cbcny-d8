{%
  set classes = [
    'section',
    'section--pullquote',
  ]
%}

<div class="pullquote">
  <div class="pullquote__item">{{ content.field_quote }}</div>
  {% if content.field_attribution|render %}
    <div class="pullquote__attribution">{{ content.field_attribution }}</div>
  {% endif %}
</div>
